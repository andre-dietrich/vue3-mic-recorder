  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-stage-3": "^7.8.3",
    "axios": "^1.3.6",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
